{
  "routes": [
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/static/js/*", "/static/css/*", "*.json"]
  },
  "mimeTypes": {
    ".json": "text/json",
    ".js": "text/javascript",
    ".css": "text/css"
  }
}
